<br>
<div class="row">
  <div class="col-md-2"></div>
  <div class="col-md-5">
    <h2 class="text-left">

      <% if schedule == "daily" %>
        <i class="fa fa-th"></i> Daily <i class="fa fa-angle-double-right"></i> <%= display_short_date(Time.now) %>
      <% elsif schedule == "weekly" %>
        <i class="fa fa-th-large"></i> Weekly <i class="fa fa-angle-double-right"></i> 12/05/15
      <% else %>
        <i class="fa fa-square"></i> Biweekly <i class="fa fa-angle-double-right"></i> 12/05/15
      <% end %>

    </h2>
  </div>
</div>
